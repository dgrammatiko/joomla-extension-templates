{
  "$schema": "https://developer.joomla.org/schemas/json-schema/web_assets.json",
  "description": "{{extensionNamePascalCase}} assets definition",
  "license": "GPL-3.0",
  "name": "{{extensionNameLowerCase}}",
  "version": "{{version}}",
  "assets": [
    {
      "name": "template.base",
      "type": "preset",
      "dependencies": [
        "template.base#style",
        "template.base#script"
      ]
    },
    {
      "name": "template.base",
      "type": "style",
      "uri": "template.min.css"
    },
    {
      "name": "template.base",
      "type": "script",
      "uri": "template.min.js",
      "attributes": {
        "type": "module"
      }
    },
    {
      "name": "template.user",
      "type": "style",
      "uri": "user.css"
    },
    {
      "name": "template.user",
      "type": "script",
      "uri": "template.js",
      "attributes": {
        "type": "module"
      }
    }
  ]
}
